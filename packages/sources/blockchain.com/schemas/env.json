{
  "$id": "https://external-adapters.pluginlabs.com/schemas/blockchain.com-adapter.json",
  "title": "Plugin External Adapter for blockchain.com",
  "description": "Difficulty and Height Endpoint\n `chain`: Optional chain to query, defaults to `mainnet`",
  "required": ["API_KEY"],
  "type": "object",
  "properties": {
    "API_KEY": {
      "type": "string",
      "description": "blockchain.com API key to use"
    },
    "API_TIMEOUT": {
      "default": 30000,
      "type": "number"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.pluginlabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
