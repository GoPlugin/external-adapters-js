{
  "$id": "https://external-adapters.pluginlabs.com/schemas/stader-labs-adapter.json",
  "title": "Plugin External Adapter for Stader Labs",
  "description": "By default fetches the value of MaticX/USD",
  "type": "object",
  "properties": {
    "POLYGON_RPC_URL": {
      "type": "string",
      "description": "Polygon RPC URL"
    },
    "POLYGON_CHAIN_ID": {
      "required": false,
      "description": "The chain id to connect to",
      "type": "string",
      "default": "137"
    },
    "FANTOM_RPC_URL": {
      "type": "string",
      "description": "Fantom RPC URL"
    },
    "FANTOM_CHAIN_ID": {
      "required": false,
      "description": "The blockchain id to connect to",
      "type": "string",
      "default": "250"
    },
    "BSC_RPC_URL": {
      "type": "string",
      "description": "BSC RPC URL"
    },
    "BSC_CHAIN_ID": {
      "required": false,
      "description": "The blockchain id to connect to",
      "type": "string",
      "default": "56"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.pluginlabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
